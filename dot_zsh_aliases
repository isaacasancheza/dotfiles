alias uuid='python3 -m uuid'
alias cpenv='which python3 | pbcopy'
alias cdkgc='sh ~/.scripts/cdkgc.sh'
alias random='python3 -m random'
alias invoke='sh ~/.scripts/invoke.sh'
alias isonow='python3 -c "import datetime; print(datetime.datetime.now(datetime.timezone.utc).isoformat())"'
alias covhtml='uv run coverage html && open htmlcov/index.html'
