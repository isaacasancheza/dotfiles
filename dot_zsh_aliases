alias p='pnpm'
alias cc='cookiecutter gh:isaacsancheza/cookiecutters --directory'
alias brew='env PATH="${PATH//$(pyenv root)\/shims:/}" brew'
alias uuid='python -m uuid'
alias cpenv='which python | pbcopy'
alias random='python -m random'
alias invoke='sh ~/.scripts/invoke.sh'
alias shadcn='pnpm dlx shadcn@latest'
alias isonow='python -c "import datetime; print(datetime.datetime.now(datetime.timezone.utc).isoformat())"'
alias covhtml='coverage html && open htmlcov/index.html'
alias minidump='sh ~/.scripts/minidump.sh'
alias fix-permissions='sh ~/.scripts/set-acl.sh'
alias set-devcontainer='python ~/.scripts/set_devcontainer.py'
alias get-cookiecutter-variables='python ~/.scripts/get_cookiecutter_variables.py'
